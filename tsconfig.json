{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/admin/products/(components)/products/data-table.jsx", "src/app/admin/products/page.jsx", "src/app/api/import/route.js", "src/app/admin/orders/page.jsx", "src/app/admin/dashboard/page.jsx", "src/app/admin/dashboard/(components)/ChartQ.jsx", "src/app/page.tsx", "src/app/admin/dashboard/(components)/Chart.jsx", "src/app/admin/layout.tsx", "src/app/admin/dashboard/Dash.jsx", "auth.config.js", "middleware.js", "src/app/(cpnotrouted)/login.jsx"],
  "exclude": ["node_modules"]
}
